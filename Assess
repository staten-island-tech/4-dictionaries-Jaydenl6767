def late():
    actions = ["take", "take", "serve", "take", "serve", "take", "close"]
    waiting = 0
    took = 0
    start = 23


    for action in actions:
        if action == "take":
            took += 1
            waiting += 1
        elif action == "serve":
            if waiting > 0:
                waiting -= 1
        elif action == "close":
            total = start + took
            print(took, waiting, total)
            break


late()
