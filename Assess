def late():
    tickets = ["take", "take", "serve", "take", "serve", "serve", "close"]
    waiting = 0
    took = 0
    start = 23


    for action in tickets:
        if action == "take":
            took = took + 1
            waiting = waiting + 1
        elif action == "serve":
            if waiting > 0:
                waiting = waiting - 1
        elif action == "close":
            total = start + took
            print(took, waiting, total)
            break


late()
